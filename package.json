{
  "name": "billingsystem-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "db:create": "sequelize-cli db:create",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "db:status": "sequelize-cli db:migrate:status",
    "db:init": "npm run db:create && npm run db:migrate && npm run db:seed",
    "db:reset": "sequelize-cli db:drop && npm run db:init"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^17.2.0",
    "express": "^4.21.2",
    "http-errors": "~1.6.3",
    "jade": "^0.29.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "mssql": "^12.0.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.14.3",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "tedious": "^19.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
